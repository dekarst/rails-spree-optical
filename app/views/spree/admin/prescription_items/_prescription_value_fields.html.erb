<tr class='prescription_value fields' id="spree_<%= dom_id(f.object) %>"  data-hook='prescription_value' class="<%= cycle('odd', 'even')%>">
    <td class='no-border'>
        <span class='handle'></span>
        <%= f.hidden_field :id %>
    </td>
    <td class='name'><%= f.text_field :name %></td>
    <td class='actions'><%= link_to_remove_fields Spree.t(:remove), f, no_text: true %></td>
</tr>
