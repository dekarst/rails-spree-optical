<% if defined?(frontend) and frontend %>
    <li class="<%= 'disabled' if current_page.last? %>">
        <%= link_to raw(t 'views.pagination.last'), current_page.last? ? '#' : url, :remote => remote %>
    </li>
<% else %>
    <span class='last'>
        <%= link_to_unless current_page.last?, raw(t 'views.pagination.last'), url, {:remote => remote} %>
    </span>
<% end %>
